<properties>
	<!-- 生成令牌控制配置文件
	author：biaoping.yin
    CopyRight：bboss
    Date:2011.4.13
	-->
	<property name="/token/*.freepage" ws:servicePort="tokenService" 
	hessian:servicePort="tokenService"
	class="org.frameworkset.web.token.ws.TokenController" />
	<!-- 
	校验令牌服务
	-->
	<property name="/checktoken/*.freepage" ws:servicePort="checktokenService" 
	hessian:servicePort="checktokenService"
	class="org.frameworkset.web.token.ws.CheckTokenContoller" />
	
	<property name="/token/v2/*.freepage" ws:servicePort="v2tokenService" 
	hessian:servicePort="v2tokenService"
	class="org.frameworkset.web.token.ws.v2.TokenController" />
	<!-- 
	校验令牌服务
	-->
	<property name="/checktoken/v2/*.freepage" ws:servicePort="v2checktokenService" 
	hessian:servicePort="v2checktokenService"
	class="org.frameworkset.web.token.ws.v2.CheckTokenContoller" />
	
	
	<property name="/auth/v2/*.freepage" ws:servicePort="v2authorService" 
		hessian:servicePort="v2authorService"
		class="org.frameworkset.web.token.ws.v2.AuthorServiceController" />
</properties>